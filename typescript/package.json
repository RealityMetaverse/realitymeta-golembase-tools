{
  "name": "@realitymeta/arkiv-reality-nft",
  "version": "0.1.4",
  "description": "Service for interacting with Reality NFT data from Arkiv network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "golem-base-sdk": "^0.1.16"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "golem-base-sdk": ">=0.1.16"
  },
  "scripts": {
    "clean": "node -e \"const fs=require('fs');if(fs.existsSync('dist'))fs.rmSync('dist',{recursive:true})\"",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "run-query": "ts-node examples/getRealityNFTData.ts",
    "run-query:interactive": "ts-node examples/getRealityNFTData.ts --interactive",
    "bun:interactive": "bun run examples/getRealityNFTData.ts --interactive"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
